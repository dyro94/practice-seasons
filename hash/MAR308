     my %name=(
          "rohan"=>"ghosh",
          "soham"=>"ghosh",
          "raju"=>"paul",
          "tapan"=>"das"
        );
     while( ($key,$value)=each(%name))
     {   print $key.",".$value."\n";
     }
     
     print"add two element..\n";
     $name{bikash}=ghosh;
     $name{oshin}=dutta;
     while(($k,$v)=each(%name))
    {
      print $k.",".$v."\n";
    }     
